{
	"name": "atvp",
	"version": "0.0.1",
	"description": "A WordPress theme for ATVP",
	"devDependencies": {
		"autoprefixer": "^10.2.4",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.19.0",
		"postcss": "^8.2.4",
		"postcss-cli": "^8.3.1",
		"postcss-custom-media": "^8.0.0",
		"postcss-import": "^14.0.0",
		"precss": "^4.0.0",
		"stylelint": "^13.9.0",
		"stylelint-config-wordpress": "^17.0.0"
	},
	"scripts": {
		"build": "npm run stylelint & npm run postcss:main & npm run postcss:editor",
		"postcss:main": "postcss -o style.css css/style.css",
		"postcss:editor": "postcss -o editor-style.css css/editor-style.css",
		"stylelint": "stylelint css/**/*.css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hapypprime/starter-theme.git"
	},
	"author": "Happy Prime",
	"bugs": {
		"url": "https://github.com/happyprime/starter-theme/issues"
